<%= render 'form', goal_target: @goal_target %>

<%= link_to 'Cancel', admin_goal_path(@goal), class: "usa-button cancel-goal-target" %>

<script>
  $('.cancel-goal-target').on("click", function(event) {
    event.preventDefault();
    $.ajax({
      type: "GET",
      url: $(this).attr("href") + "/targets"
    });
  });
</script>
