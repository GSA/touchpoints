<p id="notice"><%= notice %></p>

<h1>Programs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Organization</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= program.organization_id %></td>
        <td><%= program.url %></td>
        <td><%= link_to 'Show', admin_program_path(program) %></td>
        <td><%= link_to 'Edit', edit_admin_program_path(program) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program', new_admin_program_path %>
