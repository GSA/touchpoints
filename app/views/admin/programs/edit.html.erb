<div class="well">
  <h1>Editing Program</h1>

  <%= render 'form', program: @program %>
</div>
<p>
  <%= link_to 'Show', admin_program_path(@program), class: "usa-button" %>
  <%= link_to 'Back', admin_programs_path, class: "usa-button usa-button--outline" %>
  <%= link_to 'Delete', admin_program_path(@program), method: :delete, data: { confirm: 'Are you sure?' }, class: "usa-button usa-button--secondary" %>
</p>
