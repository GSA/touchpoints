<div class="usa-accordion usa-accordion--bordered">
  <h4 class="usa-accordion__heading">
    <button
      class="usa-accordion__button"
      aria-expanded="false"
      aria-controls="csv-data">
      CSV data for performance.gov/cx charts
    </button>
  </h4>
  <div id="csv-data" class="usa-accordion__content">
    <h4>
      cx-summary-data.csv
    </h4>
    <p>
      <%= render "components/reporting/cx_summary_data_detail_row", omb_cx_reporting_collection: @omb_cx_reporting_collection %>
    </p>

    <h4>
      Service details
    </h4>
    <% header_fields = [
        :service_provider_id,
        :service_id,
        :year,
        :quarter,
        :standardized_question_number,
        :standardized_question_identifier,
        :customized_question_text,
        :likert_scale_1,
        :likert_scale_2,
        :likert_scale_3,
        :likert_scale_4,
        :likert_scale_5,
        :point_scale,
        :response_volume,
        :notes,
        :question_total,
        :start_date,
        :end_date
      ] %>
    <%= header_fields.join(",") %>
    <br>
    <%# WRITE A LINE FOR EACH OF THE 7 STANDARD QUESTIONS %>
    <% (1..7).each do |question_number| %>
      <%= render 'admin/collections/csv_detail_row', omb_cx_reporting_collection: omb_cx_reporting_collection, question_number: question_number %>
      <br>
    <% end %>
  </div>
</div>
<br>
