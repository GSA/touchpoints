.fba-modal-dialog {
  font-family: Source Sans Pro Web, Helvetica Neue, Helvetica, Roboto, Arial, sans-serif;
  font-size: 100%;
}

.fba-modal-dialog h1 {
  margin-right: 20px;
  margin-top: -1rem;
  word-wrap: break-word;
}

.fba-inline-container {
  background: #fff;
  border: 1px solid #E5E5E5;
  max-width: 35rem;
  margin: 0 auto 40px auto;
}

.fixed-tab-button {
  bottom: 0;
  padding: 5px 10px;
  position: fixed;
  right: 12px;
  z-index: 9999;

}
#fba-button.usa-button:hover,
.fixed-tab-button.usa-button:hover {
  color:white;
  background-color:#1a4480;
  border-bottom:0;
  text-decoration:none;
}

#fba-text-name, #fba-text-email {
  max-width: 100% !important;
  font-size: 100%
}

/* Form Sections */
.touchpoints-form-wrapper form div.section {
  display: none;
}
.touchpoints-form-wrapper form div.section.fba-visible {
  display: block;
}

.hide {
  display: none;
}

