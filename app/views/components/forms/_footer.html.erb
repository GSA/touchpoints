<hr style="border: none; border-top: 1px solid #E5E5E5;">
<div class="usa-grid-full" style="margin-bottom: 36px;">
  <div class="usa-width-one-half">
    <small id="fba-dialog-privacy" class="usa-external_link">
      <a href="https://www.gsa.gov/portal/content/162010" target="_blank">Privacy</a>
    </small>
  </div>
  <div class="usa-width-one-half">
    <div style="text-align: right;">
      <small>
        <% if touchpoint.present? %>
        <% if touchpoint.omb_approval_number.present? %>
        OMB Approval #<span class="omb-approval-number"><%= touchpoint.omb_approval_number %></span>
        <% end %>

        <% if touchpoint.expiration_date? %>
        &middot;
        Exp. Date <%= touchpoint.expiration_date.strftime("%m/%d/%Y") %>
        <% end %>
        <% else %>
        OMB Approval #<span class="omb-approval-number">00000000</span>
        &middot;
        Exp. Date MM/DD/YYYY
        <% end %>
      </small>
    </div>
  </div>
</div>
